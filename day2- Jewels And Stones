class Solution {
    public int numJewelsInStones(String J, String S) {
        int count=0;
        for(char s:S.toCharArray())
        {
            for(char j:J.toCharArray())
            {
                if(s==j)
                {
                    count++;
                    break;
                }
            }
        }
        return count;
    }
}
