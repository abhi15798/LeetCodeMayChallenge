class Solution {
    public int findComplement(int num) {
        char[]data = Integer.toBinaryString(num).toCharArray();
        String s="";
        for(int i=0;i <= data.length-1; i++)
        {
            if(data[i]=='0')
            {
                s+=""+1;
            }
            else
            {
                s+=""+0;
            }
            
        }
        int decimal=Integer.parseInt(s,2);
        return decimal;
        
    }
}
